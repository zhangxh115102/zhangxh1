body{background:#f7f7f7;height:100%}.titList{padding:0 .2rem;height:.88rem;display:-webkit-box;display:-webkit-flex;display:-moz-box;display:-ms-flexbox;display:flex;background:#fff}.titList li{-webkit-box-flex:1;-webkit-flex:1;-moz-box-flex:1;-ms-flex:1;flex:1;text-align:center}.titList li span{color:#aeaeae;font-size:.32rem;line-height:.84rem;display:inline-block}.titList .active span{color:#38bbff;border-bottom:.04rem solid #38bbff}.hidden{height:100%;overflow-x:hidden}.tabSwiper{margin-top:.3rem;overflow:visible}.tabSwiper .questionList{padding-left:.3rem;background:#fff}.tabSwiper .questionList li{background:#fff}.tabSwiper .questionList li .questionTit{height:.88rem;border-top:1px solid #e7e7e7;display:-webkit-box;display:-webkit-flex;display:-moz-box;display:-ms-flexbox;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;-moz-box-pack:justify;-ms-flex-pack:justify;justify-content:space-between;-webkit-box-align:center;-webkit-align-items:center;-moz-box-align:center;-ms-flex-align:center;align-items:center}.tabSwiper .questionList li .questionTit a{width:6.6rem;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;display:block;color:#666;font-size:.32rem;line-height:.88rem}.tabSwiper .questionList li .questionTit img{display:block;width:.14rem;height:.22rem;margin-right:.3rem;-webkit-transform:rotate(90deg);transform:rotate(90deg)}.tabSwiper .questionList li .questionTit .rotate{-webkit-animation:rotate 1s linear;animation:rotate 1s linear;-webkit-transform:rotate(270deg);transform:rotate(270deg)}.tabSwiper .questionList li .questionTit keyframes rotate 0%{-webkit-transform:rotate(0)}.tabSwiper .questionList li .questionTit keyframes rotate 100%{-webkit-transform:rotate(90deg)}.tabSwiper .questionList li .questionTit -webkit-keyframes rotate 0%{-webkit-transform:rotate(0);transform:rotate(0)}.tabSwiper .questionList li .questionTit -webkit-keyframes rotate 100%{-webkit-transform:rotate(270deg);transform:rotate(270deg)}.tabSwiper .questionList li .answer{display:none;padding:.3rem;padding-left:0;line-height:.42rem;font-size:.28rem;color:#aeaeae}.tabSwiper .questionList li .answer span{color:#38bbff}